{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "copy-data-emr"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SQlDB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQlDB_LS'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://medicaladlsg2.dfs.core.windows.net/"
		},
		"SQlDB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().serverName}"
		},
		"SQlDB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().databaseName}"
		},
		"SQlDB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Aman"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "copy-data-emr_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"SQlDB_LS_password": {
						"value": "[parameters('SQlDB_LS_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"SQlDB_LS_properties_typeProperties_server": {
						"value": "[parameters('SQlDB_LS_properties_typeProperties_server')]"
					},
					"SQlDB_LS_properties_typeProperties_database": {
						"value": "[parameters('SQlDB_LS_properties_typeProperties_database')]"
					},
					"SQlDB_LS_properties_typeProperties_userName": {
						"value": "[parameters('SQlDB_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}