					},
					"dataset": {
						"referenceName": "sqlDb_dataset",
						"type": "DatasetReference",
						"parameters": {
							"serverName": {
								"value": "@pipeline().parameters.serverName",
								"type": "Expression"
							},
							"databaseName": {
								"value": "@pipeline().parameters.databaseName",
								"type": "Expression"
							},
							"tableName": {
								"value": "@pipeline().parameters.tableName",
								"type": "Expression"
							}
						}
					}
				}
			}
		],
		"parameters": {
			"serverName": {
				"type": "string",
				"defaultValue": "sqldb-serve.database.windows.net"
			},
			"databaseName": {
				"type": "string",
				"defaultValue": "hospital-a"
			},
			"tableName": {
				"type": "string",
				"defaultValue": "IncrementalLoadControl"
			}
		},
		"variables": {
			"LastLoadTimestamp": {
				"type": "String"
			}
		},
		"annotations": []
	}
